<div class="menu-item-inner {{#if _isLocked}}locked{{/if}}" aria-label="{{_globals._menu._deltaBoxMenu.menuItem}}" {{#if _globals._menu._deltaBoxMenu.menuItem}}tabindex="0"{{/if}}>
    <div class="menu-item-graphic">
        {{#if _graphic.src}}
            {{#if _isLocked}}
              <img src="{{menuSettings._deltaBoxMenu._graphic.locked}}" alt="{{menuSettings._deltaBoxMenu._graphic.alt}}" />
            {{else}}
              <a href="#/id/{{_id}}" class="{{#if _isVisited}}visited{{/if}}">
                  <img src="{{_graphic.src}}" alt="{{_graphic.alt}}" />
              </a>
            {{/if}}
        {{/if}}
    </div>
    <div class="menu-item-title">
        <div class="menu-item-title-inner h3 accessible-text-block" role="heading" aria-level="2" tabindex="0">{{{displayTitle}}}</div>
    </div>
    <div class="menu-item-body">
        <div class="menu-item-body-inner">{{{a11y_text body}}}</div>
    </div>
    <div class="menu-item-button">
        <button aria-label="{{linkText}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" class="{{#if _isVisited}}visited{{/if}} {{#if _isLocked}}disabled{{/if}} accessible-text-block" {{#if _isLocked}}disabled="disabled"{{/if}}>
            {{#if _isLocked}}
            {{{menuSettings._deltaBoxMenu.linkLockedText}}}
                {{else}}
                {{#if _isVisited}}
                  {{{menuSettings._deltaBoxMenu.linkVisitedText}}}
                {{else}}
                  {{{linkText}}}
                {{/if}}
            {{/if}}
        </button>
    </div>
    {{#if duration}}
        <div class="menu-item-duration accessible-text-block" role="region" tabindex="0">{{#if _globals._menu._deltaBoxMenu.durationLabel}}{{{_globals._menu._deltaBoxMenu.durationLabel}}}{{/if}} {{{menuSettings._deltaBoxMenu.durationLabel}}} {{{duration}}}</div>
    {{/if}}
</div>
